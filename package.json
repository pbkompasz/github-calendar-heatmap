{
  "name": "github-calendar-heatmap",
  "version": "1.0.0",
  "description": "A lightweight calendar heatmap Vue 3 component built on SVG, inspired by github's contribution calendar graph",
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "vue3js",
    "vue-component",
    "vue3-component",
    "calendar",
    "heatmap"
  ],
  "main": "dist/github-calendar-heatmap.umd.js",
  "module": "dist/github-calendar-heatmap.es.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbkompasz/github-heatmap.git"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs --clean-cache",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "tippy.js": "^6.3.7",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/node": "^16.11.24",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vuepress/client": "2.0.0-beta.59",
    "@vuepress/plugin-register-components": "^2.0.0-beta.35",
    "@vuepress/plugin-shiki": "^2.0.0-beta.35",
    "sass": "^1.49.7",
    "typescript": "^4.5.5",
    "vite": "^2.8.1",
    "vite-plugin-dts": "^0.9.9",
    "vitest": "^0.28.2",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.12",
    "vuepress": "2.0.0-beta.59"
  },
  "peerDependencies": {
    "vue": "^3.2.29"
  },
  "engines": {
    "node": ">=16"
  }
}
